    script(src='http://hotelsquare.azurewebsites.net/js/bootstrap.min.js')
    script(src='http://hotelsquare.azurewebsites.net/js/responsiveslides.min.js')      
    script(type='text/javascript')
      $('body').show();      
      NProgress.start();
    script(src='http://code.jquery.com/ui/1.10.3/jquery-ui.js')
    script(src='http://hotelsquare.azurewebsites.net/js/holder.js')
    script(src='http://hotelsquare.azurewebsites.net/js/typeahead.js')
